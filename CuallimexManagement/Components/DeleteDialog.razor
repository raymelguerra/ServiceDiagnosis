<MudDialog>
    <DialogContent>
        <MudText> Desea eliminar el diagnóstico con Folio: @TitleLabel</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Cancel</MudButton>
        <MudButton Color="Color.Error" Variant="Variant.Filled" OnClick="Submit">Eliminar</MudButton>
    </DialogActions>
</MudDialog>
@code {
    [Parameter] public string? TitleLabel { get; set; }
    [CascadingParameter] MudDialogInstance MudDialog { get; set; }

    void Submit() => MudDialog.Close(DialogResult.Ok(true));
    void Cancel() => MudDialog.Cancel();
}